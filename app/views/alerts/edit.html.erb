<h1>Editing alert</h1>

<% form_for(@alert) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :alert_date %><br />
    <%= f.text_field :alert_date %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :contract_number %><br />
    <%= f.text_field :contract_number %>
  </p>
  <p>
    <%= f.label :contract_title %><br />
    <%= f.text_field :contract_title %>
  </p>
  <p>
    <%= f.label :contract_id %><br />
    <%= f.text_field :contract_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @alert %> |
<%= link_to 'Back', alerts_path %>