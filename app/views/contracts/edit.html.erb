<h1>Editing contract</h1>

<% form_for(@contract) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :number %><br />
    <%= f.text_field :number %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :signature_date %><br />
    <%= f.text_field :signature_date %>
  </p>
  <p>
    <%= f.label :effective_date %><br />
    <%= f.text_field :effective_date %>
  </p>
  <p>
    <%= f.label :expiration_date %><br />
    <%= f.text_field :expiration_date %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @contract %> |
<%= link_to 'Back', contracts_path %>